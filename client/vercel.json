{
  "version": 2,
  "builds": [
    {
      "src": "client/dist/index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/HyperVault/(.*)",
      "dest": "/HyperVault/client/dist/$1"
    },
    {
      "src": "/HyperVault",
      "dest": "/HyperVault/client/dist/index.html"
    }
  ],
  "headers": [
    {
      "source": "/HyperVault/client/static/js/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    }
  ]
}
